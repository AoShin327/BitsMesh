# ================================
# Vanilla 3.3 深度定制版 .gitignore
# ================================

# 环境配置 - 每个环境不同
conf/config.php

# 用户数据 - 不应提交
uploads/*
!uploads/.gitkeep

# 缓存 - 自动生成
cache/*
!cache/.gitkeep

# 系统文件
.DS_Store
*.log
Thumbs.db

# IDE
.idea/
.vscode/
*.swp
*.swo

# 依赖 (完整Fork模式下跟踪vendor)
# /vendor/

# Playwright 测试产物
themes/bitsmesh/tests/test-results/
themes/bitsmesh/tests/playwright-report/
themes/bitsmesh/tests/playwright/.cache/

# Node 依赖
node_modules/

# 临时文件
*.tmp
*.bak

# Claude Code 会话数据
.claude/
.spec-workflow/
CLAUDE.md
**/CLAUDE.md
docs/

# 临时快照
web-snap/
nodeseek-web-snap/

# 临时截图
*.png
!themes/**/images/*.png
!plugins/**/images/*.png

# Playwright MCP 临时目录
.playwright-mcp/

# 本地路由配置
router.php

# Apache 本地配置
.htaccess
